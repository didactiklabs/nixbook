apiVersion: v1
clusters:
- cluster:
    certificate-authority-data: 
    server: https://iaas.didactik.labs:6443
  name: iaas
contexts:
- context:
    cluster: iaas
    user: oidc
  name: oidc@iaas.didactik.labs
current-context: oidc@iaas.didactik.labs
kind: Config
preferences: {}
users:
- name: oidc
  user:
    exec:
      apiVersion: client.authentication.k8s.io/v1beta1
      args:
      - oidc-login
      - get-token
      - --oidc-issuer-url=https://auth.didactik.labs/application/o/kubernetes/
      - --oidc-client-id=kubernetes
      - --oidc-extra-scope=email,oidc,profile,groups
      command: kubectl
      env: null
      provideClusterInfo: false
