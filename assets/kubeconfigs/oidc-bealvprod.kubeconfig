apiVersion: v1
clusters:
- cluster:
    certificate-authority-data: 
    server: https://10.250.0.13:6443
  name: bealv-4c2fn
contexts:
- context:
    cluster: bealv-4c2fn
    user: oidc
  name: oidc@bealv-4c2fn
current-context: oidc@bealv-4c2fn
kind: Config
preferences: {}
users:
- name: oidc
  user:
    exec:
      apiVersion: client.authentication.k8s.io/v1beta1
      args:
      - oidc-login
      - get-token
      - --oidc-issuer-url=https://zitadel.bealv.io
